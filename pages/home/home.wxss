.search {
  width: 710rpx;
  height: 60rpx;
  position: fixed;
  margin: 0 auto;
  left: 0;
  right: 0;
  bottom: 0;
  top: 10rpx;
  background-color: #ffffff;
  border-radius: 10px;
  color: rgba(136, 136, 136, 1);
  font-size: 14px;
  text-align: left;
  font-family: Arial;
  border: 1px solid rgba(187, 187, 187, 1);
  z-index: 999;
}
.search icon
{
  position: absolute;
  top: 10rpx;
  left: 10rpx;
}
.search input
{
  
  height: 60rpx;
  line-height: 60rpx;
  vertical-align: middle;
  padding-left: 50rpx;
}


swiper {
  width: 100%;
  height: 375rpx;
}

swiper image {
  width: 100%;
  height: 375rpx;
}

image {
  background-color: #ddd;
}
